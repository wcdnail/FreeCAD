{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++WINGDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.chacha/bin/FreeCAD.exe",
            "args": [
                "--log-file log.txt",
                "-u config/user.cfg",
                "-s config/system.cfg"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/.chacha",
            "environment": [],
            "externalConsole": true,
            "pipeTransport": {
                "debuggerPath": "${env:MSYS_ROOT}/clang64/bin/gdb.exe",
                "pipeProgram": "${env:MSYS_ROOT}/usr/bin/bash.exe",
                "pipeArgs": ["-c"],
                "pipeCwd": ""
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "C++VSDBG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.chacha/bin/FreeCAD.exe",
            "args": [
                "--log-file log.txt",
                "-u config/user.cfg",
                "-s config/system.cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/.chacha",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}