{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++WINGDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.chacha-debug/bin/FreeCAD.exe",
            "args": [
                "--log-file", "log.txt",
                "-u", "config/user.cfg",
                "-s", "config/system.cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/.chacha-debug",
            "environment": [
                { "name": "MINGW_PREFIX", "value": "${env:MSYS_ROOT}/clang64" },
                { "name": "FC_PYTHON_STDLIB", "value": "C:/msys/clang64/lib/python311.zip;C:/msys/clang64/lib/python3.11;D:/a/msys64/clang64/lib/python3.11/lib-dynload" },
                { "name": "VS_CODE_LAUNCH_HINT", "value": "https://code.visualstudio.com/docs/editor/variables-reference#_environment-variables" }
            ],
            "externalConsole": true,
            "pipeTransport": {
                "debuggerPath": "${env:MSYS_ROOT}/clang64/bin/gdb.exe",
                "pipeProgram": "${env:MSYS_ROOT}/usr/bin/bash.exe",
                "pipeArgs": ["-c"],
                "pipeCwd": ""
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "C++VSDBG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/.chacha-debug/bin/FreeCAD.exe",
            "args": [
                "--log-file", "log.txt",
                "-u", "config/user.cfg",
                "-s", "config/system.cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/.chacha-debug",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}